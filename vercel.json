{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    },
    {
      "src": "front3/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    },
    {
      "src": "/(.*)",
      "dest": "front3/$1"
    }
  ]
}
